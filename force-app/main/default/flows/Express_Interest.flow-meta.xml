        <rules>
            <name>External_Grants</name>
            <conditionLogic>(1 AND (2 OR 3) AND 4)</conditionLogic>
            <conditions>
                <leftValueReference>Type</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Grant</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>ApplicationLimit</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>ApplicationLimit</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>NoticeofIntentRequired</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>External_Grant_assgn</targetReference>
            </connector>
            <label>External Grants</label>
        </rules>
        <rules>
            <name>Proposal</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Type</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Tender/RFQ</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>SubType</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Commercialisation</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Proposal_assgn</targetReference>
            </connector>
            <label>Proposal</label>
        </rules>
        <rules>
            <name>Contract_Client_Template</name>
            <conditionLogic>(1 AND 2)</conditionLogic>
            <conditions>
                <leftValueReference>Type</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Quote/Proposal</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>ClientSuppliedTemplate</leftValueReference>

            <name>originalFormula</name>
            <value>
                <stringValue>DATE (
 
    /*YEAR*/
    YEAR ([Ethics_Request__c].I_RS_Ethics_Application_Expiry_Date__c) - FLOOR(5 / 12) - IF (MONTH ([Ethics_Request__c].I_RS_Ethics_Application_Expiry_Date__c) - MOD(5, 12) &gt; 0, 0, 1),
        

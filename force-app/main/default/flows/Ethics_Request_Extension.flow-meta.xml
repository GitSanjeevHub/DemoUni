        CASE (
        /* Month */
        IF (MONTH ([Ethics_Request__c].I_RS_Ethics_Application_Expiry_Date__c) - MOD (5, 12) &gt; 0, MONTH ([Ethics_Request__c].I_RS_Ethics_Application_Expiry_Date__c) - MOD (5, 12), MONTH ([Ethics_Request__c].I_RS_Ethics_Application_Expiry_Date__c) - MOD (5, 12) + 12), 9, 30, 4, 30, 6, 30, 11, 30, 2, 

<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <submitter>GRS</submitter>
        <type>role</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>I_RS_Form_Details_Help_Text__c</field>
        <field>I_RS_Student_Full_Name__c</field>
        <field>Name</field>
        <field>I_RS_Degree__c</field>
        <field>I_RS_Field_of_Research__c</field>
        <field>I_RS_Anticipated_submission_date__c</field>
        <field>I_RS_Thesis_Title__c</field>
        <field>I_RS_Thesis_abstract__c</field>
        <field>I_RS_Submission_Help_Text__c</field>
        <field>I_RS_Dummy_Field_Approval__c</field>
        <field>I_RS_Item_to_be_submitted_1__c</field>
        <field>I_RS_Submission_date_5__c</field>
        <field>I_RS_Item_to_be_submitted_2__c</field>
        <field>I_RS_Submission_date_1__c</field>
        <field>I_RS_Item_to_be_submitted_3__c</field>
        <field>I_RS_Submission_date_2__c</field>
        <field>I_RS_Item_to_be_submitted_4__c</field>
        <field>I_RS_Submission_date_3__c</field>
        <field>I_RS_Item_to_be_submitted_5__c</field>
        <field>I_RS_Submission_date_4__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>I_RS_Supervisor_Approver__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Approval from Principal Supervisor</label>
        <name>Approval_from_Principal_Supervisor</name>
    </approvalStep>
    <emailTemplate>Thesis_Submissions/Seek_Approval_for_Notice_Of_Intention</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>NOI_to_Submit__c.I_RS_Status__c</field>
            <operation>equals</operation>
            <value>GRS Processing</value>
        </criteriaItems>
        <criteriaItems>
            <field>NOI_to_Submit__c.I_RS_Supervisor_Approver__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>NOI_Approved_candidate</name>
            <type>Alert</type>
        </action>
        <action>
            <name>NOI_Approved_DGR</name>
            <type>Alert</type>
        </action>
        <action>
            <name>NOI_Approved_Supervisor</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Update_NOI_Submit_Status_To_Initiate</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>NOI_Rejection</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Set_NOI_Status_To_Draft</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Update_NOI_Submit_Status_To_Approvals</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>NOI SUBMIT APPROVAL_v2</label>
    <processOrder>1</processOrder>
    <recallActions>
        <action>
            <name>Update_NOI_Submit_Status_To_GRS_Processi</name>
            <type>FieldUpdate</type>
        </action>
    </recallActions>
    <recordEditability>AdminOnly</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>

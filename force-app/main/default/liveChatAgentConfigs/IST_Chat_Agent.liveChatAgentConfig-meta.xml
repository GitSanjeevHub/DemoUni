        <users>
            <user>demo@uni.com</user>
        </users>
    </assignments>
    <autoGreeting>Hi {!LiveAgent_VisitorName}</autoGreeting>
    <criticalWaitTime>60</criticalWaitTime>
    <disableTransferConferenceGreeting>false</disableTransferConferenceGreeting>
    <enableAgentFileTransfer>true</enableAgentFileTransfer>
    <enableAgentSneakPeek>false</enableAgentSneakPeek>

    <fullName>End_Date_Formula__c</fullName>
    <externalId>false</externalId>
    <formula>IF(LEN(TEXT(DAY(EndDate))) = 1, &quot;0&quot; &amp; TEXT(DAY(EndDate)), TEXT(DAY(EndDate)))
&amp; &quot;-&quot; &amp; 
IF(LEN(TEXT(MONTH(EndDate))) = 1, &quot;0&quot; &amp; TEXT(MONTH(EndDate)),TEXT(MONTH(EndDate)))

&amp; &quot;-&quot; &amp; 


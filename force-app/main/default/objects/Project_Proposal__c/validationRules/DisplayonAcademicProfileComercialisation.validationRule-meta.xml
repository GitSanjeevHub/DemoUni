<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DisplayonAcademicProfileComercialisation</fullName>
    <active>true</active>
    <errorConditionFormula>AND( 
     ISPICKVAL(I_RS_Project_Sub_type__c,&apos;Commercialisation&apos;),
     NOT(ISPICKVAL(I_RS_Display_on_Researcher_Profile__c,&quot;Not Specified&quot;))
   )</errorConditionFormula>

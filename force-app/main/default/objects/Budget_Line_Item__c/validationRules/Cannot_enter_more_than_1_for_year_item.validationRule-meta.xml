<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_enter_more_than_1_for_year_item</fullName>
    <active>true</active>
    <errorConditionFormula>TEXT(I_RS_Unit_of_Time__c) = &apos;Years&apos; &amp;&amp;
(Year_1_Quantity__c &gt; 1 ||
Year_2_Quantity__c &gt; 1 ||
Year_3_Quantity__c &gt; 1 ||
Year_4_Quantity__c &gt; 1 ||
Year_5_Quantity__c &gt; 1)</errorConditionFormula>
    <errorDisplayField>I_RS_Unit_of_Time__c</errorDisplayField>
    <errorMessage>Cannot enter more than 1 year per Units column</errorMessage>
</ValidationRule>

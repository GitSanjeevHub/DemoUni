<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_enter_more_than_12_for_month_item</fullName>
    <active>true</active>
    <errorConditionFormula>TEXT(I_RS_Unit_of_Time__c) = &apos;Months&apos; &amp;&amp;
(Year_1_Quantity__c &gt; 12 || 
Year_2_Quantity__c &gt; 12 ||
Year_3_Quantity__c &gt; 12 ||
Year_4_Quantity__c &gt; 12 ||
Year_5_Quantity__c &gt; 12)</errorConditionFormula>
    <errorDisplayField>I_RS_Unit_of_Time__c</errorDisplayField>
    <errorMessage>Cannot enter more than 12 months per Units column</errorMessage>
</ValidationRule>

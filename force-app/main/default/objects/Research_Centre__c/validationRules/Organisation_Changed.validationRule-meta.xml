<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Organisation_Changed</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISNEW()),
(ISCHANGED( I_RS_Organisation__c ))
)</errorConditionFormula>
    <errorDisplayField>I_RS_Organisation__c</errorDisplayField>
    <errorMessage>Cannot change Research Centre</errorMessage>
</ValidationRule>

<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Expected_Completion_Date_less_than_Today</fullName>
    <active>true</active>
    <description>Expected Completion Date can&apos;t be a date in past</description>
    <errorConditionFormula>I_RS_Expected_Completion_Date__c &lt; Today()</errorConditionFormula>
    <errorDisplayField>I_RS_Expected_Completion_Date__c</errorDisplayField>
    <errorMessage>Expected Completion Date can&apos;t be less than today&apos;s date</errorMessage>
</ValidationRule>

<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>I_RM_La_Trobe_Staff_IMG__c</fullName>
    <description>This field is used to display a green tick icon if I_RM_La_Trobe_Staff_c is greater than 1, or a red cross</description>
    <externalId>false</externalId>
    <formula>IF(  I_RM_La_Trobe_Staff__c  &gt;0, IMAGE(&quot;/resource/1584504556000/GreenCheck&quot;,&quot;GreenCheck&quot;,20,20), IMAGE(&quot;/resource/1639543524000/RedCross&quot;,&quot;Red Cross&quot;,20,20))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Indicates whether the Contact is a current La Trobe Staff member.</inlineHelpText>
    <label>La Trobe Staff</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>

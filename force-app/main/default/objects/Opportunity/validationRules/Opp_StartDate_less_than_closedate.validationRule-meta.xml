<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Opp_StartDate_less_than_closedate</fullName>
    <active>true</active>
    <errorConditionFormula>L_PR_StartDate__c &gt; CloseDate</errorConditionFormula>
    <errorDisplayField>L_PR_StartDate__c</errorDisplayField>
    <errorMessage>Opportunity Start date should not be greater than Opportunity Close Date</errorMessage>
</ValidationRule>
